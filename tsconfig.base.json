{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "suppressImplicitAnyIndexErrors": true,
    "strictPropertyInitialization": true,
    "strict": true,
    "strictNullChecks": true,
    "declaration": true,
    "removeComments": true
  }
}
