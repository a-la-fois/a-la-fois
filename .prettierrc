{
  "overrides": [
    {
      "files": ["*.js", "*.ts"],
      "options": {
        "trailingComma": "es5",
        "tabWidth": 4,
        "useTabs": false,
        "singleQuote": true,
        "printWidth": 120
      }
    },
    {
      "files": "*.json",
      "options": {
        "tabWidth": 2
      }
    }
  ]
}
