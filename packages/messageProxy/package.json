{
  "name": "@a-la-fois/message-proxy",
  "version": "0.0.20",
  "description": "",
  "license": "MIT",
  "author": "",
  "files": [
    "lib"
  ],
  "main": "./lib/cjs/library/index.js",
  "module": "./lib/esm/library/index.js",
  "types": "./lib/esm/library/index.d.ts",
  "exports": {
    "require": "./lib/cjs/library/index.js",
    "import": "./lib/esm/library/index.js",
    "types": "./lib/esm/library/index.d.ts"
  },
  "scripts": {
    "build": "yarn build:lib && yarn build:server",
    "build:server": "nest build",
    "build:lib": "tsc --project ./tsconfig.build.lib.cjs.json && tsc --project ./tsconfig.build.lib.esm.json",
    "pub": "echo proxy",
    "prebuild:server": "rimraf dist",
    "prebuild:lib": "rimraf lib",
    "prepare": "npm run build:lib",
    "publish:image": "./scripts/dockerPublish.sh",
    "clear": "rimraf dist lib",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch --preserveWatchOutput",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:dapr": "dapr run --app-id messageProxy --app-port 3000 --dapr-http-port 3500 --placement-host-address localhost:50006 --components-path ../../daprComponents/dev yarn start",
    "start:dapr:test": "dapr run --app-id messageProxy --app-port 3000 --dapr-http-port 3500 --placement-host-address localhost:50006 --components-path ../../daprComponents/dev node test",
    "start:dapr:dev": "dapr run --app-id messageProxy --app-port 3000 --dapr-http-port 3500 --placement-host-address localhost:50006 --components-path ../../daprComponents/dev yarn start:dev",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublish": "clean-package --clean",
    "postpublish": "clean-package --restore",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@a-la-fois/doc-handler": "0.0.20",
    "@a-la-fois/nest-common": "0.0.20",
    "@a-la-fois/pubsub": "0.0.20",
    "@dapr/dapr": "3.0.0",
    "@nestjs/common": "9.4.2",
    "@nestjs/config": "2.3.2",
    "@nestjs/core": "9.4.2",
    "@nestjs/platform-express": "9.4.2",
    "@nestjs/platform-ws": "9.4.2",
    "@nestjs/websockets": "9.4.2",
    "reflect-metadata": "0.1.13",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@a-la-fois/clean-package": "0.0.20",
    "@nestjs/cli": "9.5.0",
    "@nestjs/schematics": "9.2.0",
    "@nestjs/testing": "9.4.2",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.2",
    "@types/node": "20.2.5",
    "@types/supertest": "2.0.12",
    "@types/uuid": "9.0.1",
    "jest": "29.5.0",
    "rimraf": "5.0.1",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "9.4.3",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
